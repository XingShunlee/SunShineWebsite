@{
    ViewBag.Title = "NewPassword";
    Layout = null;
}
<link rel="stylesheet" href="../common/css/websitebase.css" /><!--基本样式应用-->
<link rel="stylesheet" href="../common/css/personal/NewPassword.css" /> <!--主体样式应用-->
<script src="../Scripts/NewPassword.js"></script>
<link rel="stylesheet" href="../common/css/CommonDialog.css" />
    <script type="text/javascript" src="../common/js/CommonDialog.js"></script>
<form id="msform">
	<!-- progressbar -->
	<ul id="progressbar">
		<li class="active">验证用户</li>
		<li>邮件信息</li>
		<li>修改成功</li>
	</ul>
	<!-- fieldsets -->
	<fieldset>
		<h2 class="fs-title">验证用户</h2>
		<h3 class="fs-subtitle">第一步</h3>
		<input type="text" name="email" id="email" placeholder="Email" />
		<input type="password" name="pass" id="pass" placeholder="Password" />
		<input type="password" name="cpass" id="cpass" placeholder="Confirm Password" />
		<input type="button" name="next" class="next action-button" value="下一步" />
        <input type="button" name="sendemail" id="sendemail" class="action-button" value="发送邮件" />
	</fieldset>
	<fieldset>
		<h2 class="fs-title">身份验证</h2>
		<h3 class="fs-subtitle">信息核对</h3>
		<input type="text" name="SHAMSG" id="SHAMSG" placeholder="填写收到的字符串" />
		<input type="button" name="previous" class="previous action-button" value="上一步" />
		<input type="button" name="next" class="next action-button" value="下一步" />
	</fieldset>
	<fieldset>
		<h2 class="fs-title">激活</h2>
		<h3 class="fs-subtitle" id="notice">激活新密码</h3>
		<input type="button" name="previous" class="previous action-button" value="上一步" />
		<input type="submit" name="submit" class="submit action-button" value="激活" />
	</fieldset>
</form>


