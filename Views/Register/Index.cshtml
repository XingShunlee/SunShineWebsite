@{
    ViewBag.Title = "会员注册";
   
}

<link rel="stylesheet" href="../css/register/register.css" />
<script src="../js/Register/register.js"></script>
<script type="text/javascript" src="../js/DaojishiPlug.js"></script>
<link rel="stylesheet" href="../css/CommonDialog.css" />
<script type="text/javascript" src="../js/CommonDialog.js"></script>

<!-- 顶部导航区域 -->
	<div class="page-body">
		<div id="left-right-layout">
			<!-- 区域顶部-->
			<div class="block-top"></div>

			<!-- 区域主侧 -->
			<div class="block-main">
               
                <div class="register-block">
                    <div class="error-tooltip"> <span class="error-text"></span></div>
                    <div class="register-typebtn"><a href="javascript:void(0);" id="mobileRegister" class="register-btnselect">MobileRegister</a><a href="javascript:void(0);" class="register-btn" id="mailRegister">EMailRegister</a></div>

                    <div class="mobile-block" id="user-register_mobile">
                        <form class="unlogin-box  clear-fix">
                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-01"></span>
                                <input id="mobile" name="mobile" class="input-text" type="text" placeholder="请输入手机号码" />

                            </div>
                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-02"></span>
                                <input id="mobilePassword" name="mobilePassword" class="input-text" type="password" autocomplete="new-password" placeholder="请设置登录密码" />

                            </div>
                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-02"></span>
                                <input id="mobileRePassword" name="mobileRePassword" class="input-text" type="password" placeholder="请重复密码" />

                            </div>


                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-03"></span>
                                <input name="mobileCode" id="mobileCode" class="input-text" type="text" placeholder="请输入右边验证码" />
                                <span class="yzm-pic">
                                    <img class="J_validate_code" src="" width="88" height="32" alt="" />
                                </span>
                            </div>
                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-03"></span>
                                <input id="mobileSms" name="mobileSms" class="input-text width-20" type="text" placeholder="请输入短信验证码" />
                                <span class="code-btn" data-time="300" id="code-btn">获取验证码</span>
                            </div>
                            <div class="clearfix term-warning-block">
                                <label class="deal"><input id="mobileDeal" name="mobileDeal" type="checkbox" value="" checked="checked" /><span class="title">已经阅读并同意<a href="/static/regrules.html" title="注册协议" target="_blank">《用户注册协议》</a></span></label>
                            </div>
                            <div class="clearfix register-btn-block">
                                <a class="submit-btn" id="J_mobileRegister-btn" href="javascript:void(0)" title="">立即注册</a>
                            </div>
                            <div class="clearfix register-other">
                                <a class="register-help" href="#" target="_blank">注册遇到问题？</a>
                                <span class="go-login">已有账号>>->><a href="/Account/Login" title="">登录</a></span>
                            </div>
                        </form>

                    </div><!--mobile-->
                    <div class="mail-block" style="display:none;" id="user-register_email">
                        <form class="unlogin-box  clear-fix">
                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-01"></span>
                                <input id="email" name="email" class="input-text" type="text" placeholder="请输入邮箱号码" />
                            </div>
                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-02"></span>
                                <input id="emailPassword" name="emailPassword" class="input-text" type="password" autocomplete="new-password" placeholder="请输入登录密码" />

                            </div>
                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-02"></span>
                                <input id="emailRePassword" name="emailRePassword" class="input-text" type="password" autocomplete="new-password" placeholder="请确认密码" />

                            </div>

                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-03"></span>
                                <input id="emailCode" name="emailCode" class="input-text" type="text" placeholder="请输入验证码" />
                                <span class="yzm-pic">
                                    <img class="J_validate_code validate-code" src="" width="128" height="38" alt="" />
                                </span>

                            </div>
                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-03"></span>
                                <input id="emailSms" name="emailSms" class="input-text width-20" type="text" placeholder="请输入收到的邮箱验证码" />
                                <span class="code-btn" data-time="300" id="email-btn">获取验证码</span>
                            </div>
                            <div class="term-warning-block clear-fix focus-style">
                                <label class="deal">
                                    <input id="emailDeal" name="emailDeal" type="checkbox" checked="checked" value="" />
                                    <span class="title">已经阅读并同意<a href="/static/regrules.html" title="用户注册协议" target="_blank">《用户注册协议》</a></span>
                                </label>
                            </div>
                            <div class="register-btn-block clear-fix focus-style">
                                <a class="submit-btn" id="J_emailRegister-btn" href="javascript:void(0);" title="">立即注册</a>
                            </div>
                            <div class="clearfix register-other">
                                <a class="register-help" href="#" target="_blank">注册遇到问题？</a>
                                <span class="go-login">已有账号>>->><a href="/Account/Login" title="">登录</a></span>
                            </div>
                        </form>
                    </div><!--email-->
                </div>
            </div><!--block_main-->
			<!-- 区域右侧 -->
			<div class="block-right"></div>

			<!-- 区域底部 -->
			<div class="block-bottom"></div>
		</div>
	</div>
