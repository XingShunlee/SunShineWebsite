@{
    ViewBag.Title = "管理员密码重设";
}

<!DOCTYPE html>
<html lang="zh-CN">
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta name="renderer" content="webkit" />
  <link rel="shortcut icon" href="../../common/css/favicon.ico" type="image/x-icon" />
  <link rel="icon" href="../../common/css/favicon.ico" type="image/x-icon" />
  <title data-title="游戏分析平台-数据分析平台-ehaiker官网">密码重设-游戏分析平台-数据分析平台-ehaiker官网</title>
  <meta name="keywords" content=游戏分析平台,数据分析平台,ehaiker官网>
  <meta name="description" content=ehaiker是一个网络游戏分析平台，通过对游戏进行多维度的数据分析，让用户能在充分了解游戏过程中的难点与收益时效，为用户的“投资决策”提供必要的数据解决方案。>
  <link rel="stylesheet" href="../../common/css/websitebase.css" />
   <link rel="stylesheet" href="../../common/css/register/home-page.min.css" />
  <link rel="stylesheet" href="../../common/css/common.min.css" />
  <link rel="stylesheet" href="../../common/css/icon.min.css" />
  <link rel="stylesheet" href="../../common/css/fuceng.min.css" />
    <script src="../common/js/jquery.min.js"></script>
    <script src="../../common/js/mgrfogetpassword.js"></script>
    <link rel="stylesheet" href="../common/css/CommonDialog.css" />
    <script type="text/javascript" src="../common/js/CommonDialog.js"></script>
</head>
<body>
    <!-- 顶部导航区域 @@Html.Partial("TopBlock")-->

    <div class="page-body">
        <div id="left-right-layout">
            <!-- 区域顶部-->
            <div class="block-top"></div>

            <!-- 区域主侧 -->
            <div class="block-main">
                <div class="index-login">
                    <div class="index-login-bg"></div>
                    <div class="login-btn J_userlogin" id="user-register_mobile">
                        <form class="unlogin-box  clear-fix">
                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-01"></span>
                                <input id="mobile" name="mobile" class="input-text" type="text" placeholder="请输入手机号码" />
                                <span class="error-text"></span>
                            </div>

                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-03"></span>
                                <input name="mobileCode" id="mobileCode" class="input-text" type="text" placeholder="请输入右边5位验证码" />
                                <span class="yzm-pic">
                                    <img class="J_validate_code" src="" width="88" height="32" alt="" />
                                </span>
                            </div>
                            <div class="login-input clear-fix focus-style">
                                <span class="left-pic left-pic-03"></span>
                                <input id="mobileSms" name="mobileSms" class="input-text width-20" type="text" placeholder="请输入短信验证码" />
                                <span class="code-btn">获取验证码</span>
                            </div>
                            <div class="login-btn J_userlogin" style="margin-bottom:5px;margin-top:5px;"><a id="J_mobileForget-btn" href="javascript:void(0)" title="">立即找回</a></div>
                            <div class="login-input clear-fix focus-style" style="border:none">
                            <p class="register-other"><a class="register-help" href="#" target="_blank">遇到问题？</a></p>
                            </div>
                        </form>

                    </div><!--mobile-->
                </div>
            </div><!--block_main-->
            <!-- 区域右侧 -->
            <div class="block-right"></div>

            <!-- 区域底部 -->
            <div class="block-bottom"></div>
        </div>
    </div>
    </div>
    <!-- 底部区域 -->
    @Html.Partial("FooterBlock")
</body>
</html>
