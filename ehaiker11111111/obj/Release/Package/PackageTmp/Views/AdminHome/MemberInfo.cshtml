<body>
 <div id="Usertool">
        <a id="btnUserDelete" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-remove'">删除</a>
        <a id="btnUserEdit" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-edit'">编辑</a>
        <input id="Usertb" type="text" style="width:300px" />
  </div>
  <table id="UserGrid"></table>
  <div id="ddUserAdd" style="display:none">
      <div style="width:250px; margin:auto;">
            <p>用户名：<input class="easyui-textbox" style="width:200px" id="username" /></p>
            <p>密    码：<input class="easyui-textbox" style="width:200px" id="userpwd" /></p>
            <a id="btnUserSave" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-save'">保存</a>
      </div>
        
   </div>
   <div id="ddrestpwd" style="display:none">
      <div style="width:250px; margin:auto;">
            <p>新密码：<input class="easyui-textbox" style="width:200px" id="newpwd" /></p>
            <a id="btnUserReset" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-save'">保存</a>
      </div>
        
   </div>
